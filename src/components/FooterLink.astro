---
import type { LinkType } from "../types";
import Link from "./Link.astro";

export interface Props {
  title: string;
  links: LinkType[];
}

const { title, links } = Astro.props;
---

<div class="mb-4 md:mb-0 md:flex md:justify-center">
  <div>
    <h2 class="font-bold text-lg dark:text-white mb-2">
      {title}
    </h2>
    {
      links.map((link) => (
        <div class="mb-2">
          <Link {...link} />
        </div>
      ))
    }
  </div>
</div>
