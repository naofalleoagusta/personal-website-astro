{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "lib": ["es2020"],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-jsx"
  }
}
